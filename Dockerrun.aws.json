{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "redash-server",
            "image": "redash/redash:7.0.0.b18042",
            "command": [
                "server"
            ],
            "environment": [
                {
                    "name": "PYTHONUNBUFFERED",
                    "value": "0"
                },
                {
                    "name": "REDASH_LOG_LEVEL",
                    "value": "INFO"
                },
                {
                    "name": "REDASH_REDIS_URL",
                    "value": "redis://test-env-1.ecok6q.0001.apne1.cache.amazonaws.com:6379/0"
                },
                {
                    "name": "REDASH_DATABASE_URL",
                    "value": "postgresql://postgres:postgres/test-env-1.c6zo0ypnrcfc.ap-northeast-1.rds.amazonaws.com:5432/postgres"
                },
                {
                    "name": "REDASH_RATELIMIT_ENABLED",
                    "value": "false"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/aws/elasticbeanstalk/Test2-env-1/redash",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "server-"
                }
            },
            "portMappings": [
                {
                    "hostPort": 5000,
                    "containerPort": 5000
                },
                {
                    "hostPort": 5678,
                    "containerPort": 5678
                }
            ],
            "essential": true,
            "memory": 512
        },
        {
            "name": "redash-worker",
            "image": "redash/redash:7.0.0.b18042",
            "essential": true,
            "memory": 512,
            "dependsOn": [
                {
                    "containerName": "redash-server",
                    "condition": "START"
                }
            ],
            "command": [
                "scheduler"
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/aws/elasticbeanstalk/Test2-env-1/redash",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "worker-"
                }
            },
            "environment": [
                {
                    "name": "PYTHONUNBUFFERED",
                    "value": "0"
                },
                {
                    "name": "REDASH_LOG_LEVEL",
                    "value": "INFO"
                },
                {
                    "name": "REDASH_REDIS_URL",
                    "value": "redis://test-env-1.ecok6q.0001.apne1.cache.amazonaws.com:6379/0"
                },
                {
                    "name": "REDASH_DATABASE_URL",
                    "value": "postgresql://postgres:postgres/test-env-1.c6zo0ypnrcfc.ap-northeast-1.rds.amazonaws.com:5432/postgres"
                },
                {
                    "name": "REDASH_RATELIMIT_ENABLED",
                    "value": "false"
                },
                {
                    "name": "QUEUES",
                    "value": "queries,scheduled_queries,celery,schemas"
                },
                {
                    "name": "WORKERS_COUNT",
                    "value": "2"
                }
            ]
        }
    ]
}


